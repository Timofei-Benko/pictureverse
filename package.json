{
  "name": "pirctureverse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "download-schema": "npx apollo service:download --endpoint=http://localhost:6969/graphql graphql-schema.json",
    "schema:codegen": "yarn run apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes='./**/*.ts' --tagName=gql --addTypename --globalTypesFile=types/graphql-global-types.ts types",
    "types:codegen": "yarn run apollo client:codegen --localSchemaFile=graphql-schema.json --includes='queries/**/*.ts' --target=typescript"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "graphql": "15.5.3",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@types/react-redux": "^7.1.23",
    "apollo": "^2.33.9",
    "autoprefixer": "^10.4.4",
    "classnames": "^2.3.1",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "postcss": "^8.4.12",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  },
  "resolutions": {
    "graphql": "15.5.3"
  },
  "description": "This is a [Next.js](https://nextjs.org/) project bootstrapped with [`create-next-app`](https://github.com/vercel/next.js/tree/canary/packages/create-next-app).",
  "main": "apollo.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Timofei-Benko/pictureverse.git"
  },
  "keywords": [],
